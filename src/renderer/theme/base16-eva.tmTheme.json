{
  "author": "Template: Chris Kempson, Scheme: kjakapat (https://github.com/kjakapat)",
  "name": "Base16 Eva",
  "semanticClass": "theme.base16.eva",
  "colorSpaceName": "sRGB",
  "gutterSettings": {
    "background": "#3d566f",
    "divider": "#3d566f",
    "foreground": "#55799c",
    "selectionBackground": "#4b6988",
    "selectionForeground": "#7e90a3"
  },
  "settings": [
    {
      "settings": {
        "background": "#2a3b4d",
        "caret": "#9fa2a6",
        "foreground": "#9fa2a6",
        "invisibles": "#55799c",
        "lineHighlight": "#55799c55",
        "selection": "#4b6988"
      }
    },
    {
      "name": "Text",
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#9fa2a6"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#55799c"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition.string",
        "punctuation.definition.variable",
        "punctuation.definition.string",
        "punctuation.definition.parameters",
        "punctuation.definition.string",
        "punctuation.definition.array"
      ],
      "settings": {
        "foreground": "#9fa2a6"
      }
    },
    {
      "name": "Delimiters",
      "scope": "none",
      "settings": {
        "foreground": "#9fa2a6"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#9fa2a6"
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#9c6cd3"
      }
    },
    {
      "name": "Variables",
      "scope": "variable",
      "settings": {
        "foreground": "#c4676c"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.require",
        "support.function.any-method"
      ],
      "settings": {
        "foreground": "#15f4ee"
      }
    },
    {
      "name": "Labels",
      "scope": "entity.name.label",
      "settings": {
        "foreground": "#bb64a9"
      }
    },
    {
      "name": "Classes",
      "scope": ["support.class", "entity.name.class", "entity.name.type.class"],
      "settings": {
        "foreground": "#ffff66"
      }
    },
    {
      "name": "Classes",
      "scope": "meta.class",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Methods",
      "scope": "keyword.other.special-method",
      "settings": {
        "foreground": "#15f4ee"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#9c6cd3"
      }
    },
    {
      "name": "Support",
      "scope": ["support.function", "support.type"],
      "settings": {
        "foreground": "#4b8f77"
      }
    },
    {
      "name": "Strings, Inherited Class",
      "scope": [
        "string",
        "constant.other.symbol",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#66ff66"
      }
    },
    {
      "name": "Integers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#ff9966"
      }
    },
    {
      "name": "Floats",
      "scope": "none",
      "settings": {
        "foreground": "#ff9966"
      }
    },
    {
      "name": "Boolean",
      "scope": "none",
      "settings": {
        "foreground": "#ff9966"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant", "constant.language"],
      "settings": {
        "foreground": "#ff9966"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#c4676c"
      }
    },
    {
      "name": "Attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#ff9966"
      }
    },
    {
      "name": "Attribute IDs",
      "scope": [
        "entity.other.attribute-name.id",
        "punctuation.definition.entity"
      ],
      "settings": {
        "foreground": "#15f4ee"
      }
    },
    {
      "name": "Selector",
      "scope": "meta.selector",
      "settings": {
        "foreground": "#9c6cd3"
      }
    },
    {
      "name": "Values",
      "scope": "none",
      "settings": {
        "foreground": "#ff9966"
      }
    },
    {
      "name": "Headings",
      "scope": [
        "markup.heading punctuation.definition.heading",
        "entity.name.section"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#15f4ee"
      }
    },
    {
      "name": "Units",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#ff9966"
      }
    },
    {
      "name": "Bold",
      "scope": ["markup.bold", "punctuation.definition.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffff66"
      }
    },
    {
      "name": "Italic",
      "scope": ["markup.italic", "punctuation.definition.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9c6cd3"
      }
    },
    {
      "name": "Code",
      "scope": "markup.raw.inline",
      "settings": {
        "foreground": "#66ff66"
      }
    },
    {
      "name": "Link Text",
      "scope": [
        "string.other.link",
        "punctuation.definition.string.end.markdown",
        "punctuation.definition.string.begin.markdown"
      ],
      "settings": {
        "foreground": "#c4676c"
      }
    },
    {
      "name": "Link Url",
      "scope": "meta.link",
      "settings": {
        "foreground": "#ff9966"
      }
    },
    {
      "name": "Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#c4676c"
      }
    },
    {
      "name": "Quotes",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#ff9966"
      }
    },
    {
      "name": "Separator",
      "scope": "meta.separator",
      "settings": {
        "background": "#4b6988",
        "foreground": "#9fa2a6"
      }
    },
    {
      "name": "Inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#66ff66"
      }
    },
    {
      "name": "Deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#c4676c"
      }
    },
    {
      "name": "Changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#9c6cd3"
      }
    },
    {
      "name": "Colors",
      "scope": "constant.other.color",
      "settings": {
        "foreground": "#4b8f77"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#4b8f77"
      }
    },
    {
      "name": "Escape Characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#4b8f77"
      }
    },
    {
      "name": "Embedded",
      "scope": ["punctuation.section.embedded", "variable.interpolation"],
      "settings": {
        "foreground": "#9c6cd3"
      }
    },
    {
      "name": "Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "background": "#c4676c",
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Broken",
      "scope": "invalid.broken",
      "settings": {
        "background": "#ff9966",
        "foreground": "#2a3b4d"
      }
    },
    {
      "name": "Deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "background": "#bb64a9",
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Unimplemented",
      "scope": "invalid.unimplemented",
      "settings": {
        "background": "#55799c",
        "foreground": "#ffffff"
      }
    }
  ],
  "uuid": "uuid"
}
